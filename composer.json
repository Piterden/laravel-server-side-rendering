{
    "name": "defr/pyrocms-server-side-rendering",
    "description": "Server side rendering JavaScript in your Laravel application",
    "type": "streams-addon",
    "keywords": [
        "pyrocms",
        "streams-platform",
        "spatie",
        "ssr",
        "laravel-server-side-rendering"
    ],
    "homepage": "https://github.com/spatie/laravel-server-side-rendering",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Denis Efremov",
            "email": "efremov.a.denis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.5.*|5.6.*",
        "spatie/server-side-rendering": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "orchestra/testbench": "~3.5|~3.6"
    },
    "autoload": {
        "psr-4": {
            "Defr\\SsrExtension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Defr\\SsrExtension\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
